<el-card class=\"box-card\">\n            <span>\n              I have completed an offer, but I did not receive my rewards\n            </span>\n            <div class=\"bold\" @click=\"clickHandler\">\n              What do I do?\n            </div>\n          </el-card>\n        </el-col>\n\n        <el-col class=\"card-section\" :md=\"12\" :sm=\"24\" :xs=\"24\">\n          <el-card class=\"box-card\">\n            <span>\n              I am missing my rewards and would like to submit a support\n              request.\n            </span>\n            <div class=\"bold\" @click=\"clickHandler\">\n              How do I submit a support request?\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n\n      <el-row class=\"divider\"></el-row>\n    </el-row>\n\n    <el-row class=\"content\">\n      <el-row class=\"description\">\n        If you have other inquiries, please contact us with the form below\n      </el-row>\n\n      <el-row class=\"form-container\">\n        <el-form ref=\"form\" :model=\"form\" :rules=\"rule\" label-position=\"top\">\n          <el-form-item label=\"What does inquiry relate to?\" prop=\"inquiry\">\n            <el-select v-model=\"form.inquiry\" placeholder=\"\">\n              <el-option\n                label=\"Missing Rewards\"\n                value=\"Missing Rewards\"\n              />\n              <el-option\n                label=\"Interested in Advertising\"\n                value=\"Interested in Advertising\"\n              />\n              <el-option\n                label=\"Interested in Becoming a Publisher\"\n                value=\"Interested in Becoming a Publisher\"\n              />\n              <el-option\n                label=\"I am a current Publisher and need support\"\n                value=\"I am a current Publisher and need support\"\n              />\n              <el-option\n                label=\"Other\"\n                value=\"Other\"\n              />\n            </el-select>\n          </el-form-item>\n\n          <el-row :gutter=\"10\">\n            <el-col :span=\"12\">\n              <el-form-item label=\"First Name\" prop=\"fName\">\n                <el-input v-model=\"form.fName\" />\n              </el-form-item>\n            </el-col>\n            <el-col :span=\"12\">\n              <el-form-item label=\"Last Name\" prop=\"lName\">\n                <el-input v-model=\"form.lName\" />\n              </el-form-item>\n            </el-col>\n          </el-row>\n\n          <el-form-item label=\"App or Website\">\n            <el-input v-model=\"form.type\"></el-input>\n          </el-form-item>\n\n          <el-form-item label=\"Skype Handle\" prop=\"type\">\n            <el-input v-model=\"form.skype\"></el-input>\n          </el-form-item>\n\n          <el-form-item label=\"Email\" prop=\"email\">\n            <el-input v-model=\"form.email\"></el-input>\n          </el-form-item>\n\n          <el-form-item label=\"Message\" prop=\"message\">\n            <el-input type=\"textarea\" v-model=\"form.message\"></el-input>\n          </el-form-item>\n\n          <el-form-item>\n            <el-row class=\"button-container\">\n              <el-button type=\"primary\" @click=\"beforeSubmit\">Submit</el-button>\n            </el-row>\n          </el-form-item>\n        </el-form>\n      </el-row>\n    </el-row>\n  </el-row>\n</template>\n\n<script>\nexport default {\n  name: 'Contact',\n  data() {\n    return {\n      form: {\n        inquiry: '',\n        fName: '',\n        lName: '',\n        type: '',\n        skype: '',\n        email: '',\n        message: ''\n      },\n      rule: {\n        inquiry: [\n          { required: true, message: 'please select', trigger: 'blur' }\n        ],\n        fName: [\n          { required: true, message: 'please type in', trigger: 'blur' }\n        ],\n        lName: [\n          { required: true, message: 'please type in', trigger: 'blur' }\n        ],\n        type: [\n          { required: true, message: 'please type in', trigger: 'blur' }\n        ],\n        email: [\n          { required: true, message: 'please type in', trigger: 'blur' }\n        ],\n        message: [\n          { required: true, message: 'please type in', trigger: 'blur' }\n        ]\n      }\n    }\n  },\n  methods: {\n    clickHandler: function () {\n      this.$router.push({\n        path: '/support'\n      })\n    },\n    beforeSubmit: function () {\n      this.$refs['form'].validate(res => {\n        console.info(res)\n        this.$notify({\n          title: '',\n          message: 'todo submit',\n          type: 'warning'\n        })\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.contact-container {\n  color: #424242;\n  text-align: center;\n  font-size: 32px;\n  font-weight: 700;\n  line-height: 60px;\n  margin-top: 40px;\n  padding: 0 2%;\n\n  .title {\n    .card-container {\n      margin-top: 40px;\n\n      .card-section {\n        margin-bottom: 16px;\n\n        color: #424242;\n        font-size: 26px;\n        font-weight: 400;\n        line-height: 31.2px;\n        text-align: center;\n\n        span {\n          opacity: 0.7;\n        }\n\n        .bold {\n          margin-top: 10px;\n          font-weight: 500;\n          opacity: 1;\n          cursor: pointer;\n        }\n      }\n    }\n\n    .divider {\n      &:before {\n        content: '';\n        width: 100%;\n        height: 1px;\n        margin-top: 20px;\n        border-top: 1px solid rgba(0, 0, 0, 0.1);\n        position: absolute;\n        left: 0;\n        top: 0;\n        z-index: 10;\n      }\n    }\n  }\n\n  .content {\n    margin-top: 80px;\n    text-align: center;\n\n    .description {\n      font-size: 18px;\n      font-weight: 500;\n      line-height: 30.6px;\n      opacity: 0.8;\n    }\n\n    .form-container {\n      max-width: 992px;\n      min-width: 375px;\n      margin: auto;\n\n      .el-select {\n        width: 100%;\n      }\n\n      .button-container {\n        margin-top: 20px;\n\n        ::v-deep .el-button {\n          &.el-button--primary {\n            background-color: #7359a4;\n            border-color: #7359a4;\n          }\n        }\n      }\n    }\n  }\n\n  ::v-deep .el-form {\n    .el-form-item {\n      margin-bottom: 8px;\n      text-align: left;\n\n      .el-form-item__label {\n        font-weight: bold;\n        color: #424242;\n      }\n    }\n  }\n}\n</style>\n"]}]}