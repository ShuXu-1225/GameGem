AAA,EAAA,KAAA,IAAA,CAAA,IADA;AAEA,QAAA,cAAA,EAAA,KAAA,IAAA,CAAA;AAFA,OAAA;AAIA,MAAA,KAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,IAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,OAAA,EAAA,GAAA,CAAA,OAFA;AAGA,YAAA,IAAA,EAAA;AAHA,WAAA;;AAMA,cAAA,QAAA,GAAA,GAAA,CAAA,MAAA;AACA,cAAA,MAAA,GAAA,QAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,YAAA,EAAA,MAAA;;AAEA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,IAAA,EAAA;AADA,WAAA;AAGA,SAdA,MAcA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,EADA;AAEA,YAAA,OAAA,EAAA,GAAA,CAAA,OAFA;AAGA,YAAA,IAAA,EAAA;AAHA,WAAA;AAKA;AACA,OAtBA;AAuBA;AA9CA;AArBA,CAAA","sourcesContent":["<template>\n  <el-row class=\"log-in-container\">\n    <el-row class=\"content\">\n      <the-logo :allow-click=\"true\" />\n\n      <el-row class=\"top\">\n        <el-row> Sign In </el-row>\n\n        <el-row class=\"linkable-container\">\n          Now Here?\n          <span class=\"linkable\" @click=\"goSignUp\">Create Account</span>\n        </el-row>\n      </el-row>\n\n      <el-row class=\"form-container\">\n        <el-form\n          ref=\"form\"\n          :model=\"form\"\n          :rules=\"rules\"\n          label-position=\"top\"\n          label-width=\"100px\"\n        >\n          <el-form-item label=\"Your Email\" prop=\"mail\">\n            <el-input v-model=\"form.mail\"></el-input>\n          </el-form-item>\n\n          <el-form-item prop=\"pwd\">\n            <div slot=\"label\">\n              <div class=\"label-container\">\n                <div>Your Password</div>\n                <div class=\"forgot-password\" @click=\"goFindPassword\">\n                  Forgot Password?\n                </div>\n              </div>\n            </div>\n\n            <el-input v-model=\"form.pwd\" type=\"password\"></el-input>\n          </el-form-item>\n        </el-form>\n      </el-row>\n\n      <el-row class=\"footer\">\n        <el-button type=\"primary\" @click=\"clickHandler\">Sign In</el-button>\n      </el-row>\n    </el-row>\n  </el-row>\n</template>\n\n<script>\nimport TheLogo from '~views/layout/components/TheLogo'\n\nimport { logIn } from '@/api'\n\nexport default {\n  name: 'LogIn',\n  components: {\n    TheLogo\n  },\n  data() {\n    return {\n      form: {\n        mail: '',\n        pwd: ''\n      },\n      rules: {\n        mail: [\n          { required: true, message: 'Email is required', trigger: 'blur' }\n        ],\n        pwd: [\n          { required: true, message: 'password is required', trigger: 'blur' }\n        ]\n      }\n    }\n  },\n  methods: {\n    clickHandler: function () {\n      this.$refs['form'].validate(valid => {\n        if (valid) this.doLogIn()\n      })\n    },\n    goSignUp: function () {\n      this.$router.push({\n        path: '/signUp'\n      })\n    },\n    goFindPassword: function () {\n      this.$notify({\n        title: '',\n        message: 'todo forgot password',\n        type: 'warning'\n      })\n    },\n    doLogIn: function () {\n      const params = {\n        playerName: this.form.mail,\n        playerPassword: this.form.pwd\n      }\n      logIn(params).then(res => {\n        if (res && res.code === 200) {\n          this.$notify({\n            title: '',\n            message: res.message,\n            type: 'success'\n          })\n\n          const loginObj = res.result\n          const userId = loginObj.id\n          window.sessionStorage.setItem('GameGemUID', userId)\n\n          this.$router.replace({\n            path: '/offerList'\n          })\n        } else {\n          this.$notify({\n            title: '',\n            message: res.message,\n            type: 'error'\n          })\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.log-in-container {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n  align-items: center;\n\n  .content {\n    width: 100%;\n    min-width: 375px;\n    max-width: 450px;\n    font-size: 21px;\n    padding: 20px;\n    font-weight: 600;\n  }\n\n  .top {\n    margin-top: 45px;\n\n    .linkable-container {\n      font-size: 16px;\n      color: rgb(181, 181, 195);\n      margin-top: 5px;\n\n      .linkable {\n        color: #7359a4;\n        cursor: pointer;\n      }\n    }\n  }\n\n  .form-container {\n    margin-top: 15px;\n\n    ::v-deep .el-form {\n      .el-form-item {\n        .el-form-item__label {\n          width: 100%;\n          font-size: 16px;\n          color: #181c32;\n\n          &::before {\n            content: ' ';\n            margin: 0;\n          }\n        }\n      }\n    }\n\n    .label-container {\n      display: flex;\n      flex-direction: row;\n\n      div {\n        /*width: calc(50vw - 20px);*/ /* padding 20px */\n      }\n\n      .forgot-password {\n        flex: 1;\n        color: #7359a4;\n        text-align: right;\n        cursor: pointer;\n      }\n    }\n  }\n\n  .footer {\n    margin-top: 15px;\n  }\n\n  ::v-deep .el-button {\n    &.el-button--primary {\n      background-color: #7359a4;\n      border-color: #7359a4;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}