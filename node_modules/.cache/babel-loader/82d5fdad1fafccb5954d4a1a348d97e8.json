>\n      <el-row class=\"menu-item\" :key=\"index\">\n        <el-radio v-model=\"radio\" :label=\"item.id\" @change=\"changeHandler\">\n          {{ item.title }}\n        </el-radio>\n      </el-row>\n    </template>\n  </el-popover>\n</template>\n\n<script>\nexport default {\n  name: 'SortMenu',\n  props: {\n    placement: {\n      type: String,\n      default: () => 'bottom'\n    }\n  },\n  data() {\n    return {\n      visible: false,\n      menuList: [\n        // sort by: 0-Most Popular 1-High to Low 2-Level of Difficulty\n        {\n          id: '0',\n          title: 'Most Popular'\n        },\n        {\n          id: '1',\n          title: 'High to Low'\n        },\n        {\n          id: '2',\n          title: 'Level of Difficulty'\n        }\n      ],\n      radio: ''\n    }\n  },\n  methods: {\n    clickHandler: function () {\n      this.visible = !this.visible\n    },\n\n    changeHandler: function (val) {\n      this.$emit('change', val)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.menu-item {\n  padding: 0 14px;\n  line-height: 44px;\n  border-bottom: 1px solid rgba(0, 0, 0, 0.03);\n  color: #666;\n  display: block;\n  cursor: pointer;\n\n  &:hover {\n    background-color: rgba(0, 0, 0, 0.03);\n    opacity: 0.7;\n  }\n}\n</style>\n"],"sourceRoot":"src/components/offerList"}]}