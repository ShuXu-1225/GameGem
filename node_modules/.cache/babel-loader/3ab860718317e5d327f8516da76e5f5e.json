keys.length) {\n    keys.map((key, index) => {\n      if (index === 0) url += `?${key}=${data[key]}`\n      if (index !== 0) url += `&${key}=${data[key]}`\n    })\n  }\n\n  return request({\n    url: url,\n    method: 'get'\n  })\n}\n"]}]}